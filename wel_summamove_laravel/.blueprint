    - database/factories/UserFactory.php
    - database/factories/ExerciseFactory.php
    - database/factories/PerformanceFactory.php
    - database/factories/ExerciseQrCodeFactory.php
    - database/factories/AuthTokenFactory.php
    - database/migrations/2025_06_19_093021_create_roles_table.php
    - database/migrations/2025_06_19_093022_create_users_table.php
    - database/migrations/2025_06_19_093023_create_exercises_table.php
    - database/migrations/2025_06_19_093024_create_performances_table.php
    - database/migrations/2025_06_19_093025_create_exercise_qr_codes_table.php
    - database/migrations/2025_06_19_093026_create_auth_tokens_table.php
